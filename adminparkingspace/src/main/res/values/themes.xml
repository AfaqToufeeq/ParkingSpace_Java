<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ParkingSpaces" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#E4E4E4</item>
        <item name="colorPrimaryVariant">@color/header_main_color</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="n">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="SmoothCheckBoxStyleContact">
        <item name="color_checked">@color/Red</item>
        <item name="color_unchecked">#EDEDED</item>
        <item name="color_unchecked_stroke">#EDEDED</item>
        <item name="stroke_width">0.1dp</item>
        <item name="color_tick">@color/white</item>
    </style>
</resources>